    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "apexcharts": "^5.3.6",
    "date-fns": "^4.1.0",
    "lucide-vue-next": "^0.562.0",
    "vue": "^3.5.26",
    "vue3-apexcharts": "^1.10.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.10.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.23",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-csp": "^1.1.2",
    "vue-tsc": "^3.2.4"
  }
