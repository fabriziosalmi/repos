{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json,css,md}\"",
    "lint": "echo 'Linting...' && exit 0"
